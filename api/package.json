{
  "name": "api",
  "version": "1.0.0",
  "description": "Kubernetes API wrapper",
  "main": "index.js",
  "author": "Arun Chalise",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-node": "nodemon --inspect dist/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "html": "cp -r ./src/public ./dist/public",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build": "npm run build-ts && npm run html"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.16.0",
    "@types/http-proxy-middleware": "^0.19.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "^10.12.16",
    "@types/rx": "^4.1.1",
    "@types/winston": "^2.4.4",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.8",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.8.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "http-proxy-middleware": "^0.19.1",
    "jsonwebtoken": "^8.4.0",
    "merge-yaml": "^1.0.0",
    "rxjs": "^6.3.3",
    "winston": "^3.1.0"
  }
}
